# RUN: toy-translate %s -emit=mlir-mid -opt -lower-pat=vector 2>&1 | FileCheck %s

def funcInputs() {
  var a<2, 3> = [1, 2, 3, 4, 5, 6];
  var b<2, 3> = [1, 2, 3, 4, 5, 6];
  var c<2, 3> = [0];
}

def add_param_input_mid_vec(a, b, c) {
  var d = a .* b;
  store(d, c);
  return;
  # CHECK-LABEL: func.func @add_param_input_mid_vec(%arg0: memref<2x3xf64>, %arg1: memref<2x3xf64>, %arg2: memref<2x3xf64>) {
  # CHECK-NEXT:    %c0 = arith.constant 0 : index
  # CHECK-NEXT:    %0 = vector.load %arg0[%c0, %c0] : memref<2x3xf64>, vector<2x3xf64>
  # CHECK-NEXT:    %1 = vector.load %arg1[%c0, %c0] : memref<2x3xf64>, vector<2x3xf64>
  # CHECK-NEXT:    %2 = arith.mulf %0, %1 : vector<2x3xf64>
  # CHECK-NEXT:    vector.store %2, %arg2[%c0, %c0] : memref<2x3xf64>, vector<2x3xf64>
  # CHECK-NEXT:    return
  # CHECK-NEXT:  }
}
